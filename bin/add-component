#!/usr/bin/env node
'use strict';

const fs = require("fs");
const args = process.argv.slice(2);

let [name, url, key] = args;
let registry = require("../bots.json");

registry[name] = {
	url: url,
	headers: {
		developer_key: key || ""
	}
}

fs.writeFileSync("./bots.json", JSON.stringify(registry));
