#!/usr/bin/env node
'use strict';

const fs = require("fs");
const args = process.argv.slice(2);

let [name, url, key] = args;
let registry = require("../components.json");

registry[name] = {
	url: url,
	headerrs: {
		developer_key: key || ""
	}
}

fs.writeFileSync("./components.json", JSON.stringify(registry));